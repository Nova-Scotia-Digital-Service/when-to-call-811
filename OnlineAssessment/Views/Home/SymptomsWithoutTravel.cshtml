@using OnlineAssessment.Resources

@{
    ViewData["Title"] = ViewBag.Title;
}

@section featured
{
    <section class="featured" id="symptomsWithoutTravel">
        <div class="row m-t-md m-b-md">
            <div class="col-md-8">
                <h1>@StringResource.Symptoms_PageTitle </h1>
            </div>
        </div>
        <div class="row m-b-md m-t-md">
            <div class="col-md-8">
                <h2>@StringResource.SymptomsWithoutTravel_H2</h2>
            </div>
        </div>
        <div class="row m-t-md">
            <div class="col-md-8">
                <a id="symptomWithoutTravelYes" class="btn btn-secondary btn-lg centered mr-4" href="@Url.Action("Call811", "Home")">@StringResource.Btn_Yes</a>
                <a id="symptomWithouTravelNo" class="btn btn-secondary btn-lg centered ml-4" href="@Url.Action("NoCallRightNow", "Home")">@StringResource.Btn_No</a>
            </div>
        </div>
    </section>
}
