@using OnlineAssessment.Resources

@{
    ViewData["Title"] = @Html.Raw(ViewBag.Title);
}

@section featured
{
    <section class="featured" id="noCallRightNow">
        <div class="row m-t-md">
            <div class="col-md-8">
                <h1>@StringResource.NoCallRightNow_PageTitle </h1>
            </div>
        </div>

        <div class="row m-b-md m-t-md">
            <div class="col-md-8">
                <h2 class="m-b-md">@StringResource.NoCallRightNow_H2</h2>
                <p class="mb-2">@StringResource.NoCallRightNow_P1</p>
                <p class="mb-2">@StringResource.NoCallRightNow_P2</p>
                <p class="mb-2">@StringResource.NoCallRightNow_P3</p>
                <p class="mb-2">@StringResource.NoCallRightNow_P4</p>
                <p class="mb-2">@StringResource.NoCallRightNow_P5</p>
                <p class="mb-2">@StringResource.Info811_1 <a href="@StringResource.CanadInfo_href">@StringResource.CanadaInfoLink</a> @StringResource.Txt_Or <a href="@StringResource.ProvinceInfo_href">@StringResource.ProvinceInfo_Link</a>@StringResource.Info811_2 <a href="tel:1-833-784-4397" class="linkStyle">1-833-784-4397</a>.</p>
            </div>
        </div>
        <div class="row m-t-md m-b-md">
            <div class="col-md-8">
                <a id="noCallRightNowBack" class="underlineLink" href="@Url.Action("Index", "Home")">@StringResource.Link_BackToStart</a>
            </div>
        </div>
    </section>
}
